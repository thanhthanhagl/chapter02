<header class="c-header">
  <div class="l-container">
    <div class="c-header__main">
      <a href="<%= BASE_URL %>index.html" class="c-header__logo"><img src="/assets/img/c2-logo.jpg" alt="Chapter02(task01)"></a>
      <nav class="c-gnav">
        <ul>
          <% if (typeof linklists != 'undefined') { %>
            <% linklists.main && linklists.main.forEach(link => { %>
              <li>
                <a <% if (link.current || link.active) { %>class="is-active"<% } %> href="<%= link.url %>"><%= link.title %></a>
                <% if (link.links) { %>
                  <ul>
                    <% link.links.forEach(child_link => { %>
                      <li>
                        <a <% if (child_link.current) { %>class="is-active"<% } %> href="<%= child_link.url %>"><%= child_link.title %></a>
                      </li>
                    <% }) %>
                  </ul>
                <% } %>
              </li>
            <% }) %>
          <% } %>
        </ul>
      </nav>
    </div>  
  </div>  
</header>
